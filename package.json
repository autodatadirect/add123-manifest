  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/runtime": "^7.10.2",
    "jest": "^24.9.0",
    "ts-standard": "^12.0.2"
  },
  "peerDependencies": {
    "amiable-forms": "^2.0.0",
